{
  "name": "GameGogakuen-UI",
  "version": "1.0.0",
  "description": "UI for Viewing GameGogakuen Stats",
  "author": "appleJax <appleJax@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.dev.js && npm start",
    "deploy": "webpack -p --config webpack.prod.js",
    "start": "node ./dist/server.js",
    "devstart": "webpack-dev-server --config webpack.dev.js --hot",
    "test": "jest --watch",
    "testOnce": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.40",
    "axios": "^0.17.1",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.33",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "babel-loader": "8.0.0-beta.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "dotenv": "^5.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest-cli": "^22.1.4",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^Actions/(.*)$": "<rootDir>/src/model/actions/$1",
      "^Components/(.*)$": "<rootDir>/src/view/components/$1",
      "^Constants/(.*)$": "<rootDir>/src/model/constants/$1",
      "^Containers/(.*)$": "<rootDir>/src/view/containers/$1",
      "^Icons/(.*)$": "<rootDir/node_modules/material-ui-icons/$1",
      "^Images/(.*)$": "<rootDir>/src/static/images/$1",
      "^Reducers/(.*)$": "<rootDir>/src/model/reducers/$1",
      "^Root/(.*)$": "<rootDir>/$1",
      "^UI/(.*)$": "<rootDir/node_modules/material-ui/$1",
      "^Utils$": "<rootDir>/src/utils/"
    }
  }
}
