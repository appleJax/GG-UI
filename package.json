{
  "name": "gamegogakuen-ui",
  "version": "1.0.0",
  "description": "UI for Viewing GameGogakuen Stats",
  "author": "appleJax <appleJax@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --mode development --config webpack.config.js && npm run copy-assets",
    "build:staging": "NODE_ENV=production webpack --mode production --config webpack.staging.js && npm run copy-assets",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack.prod.js && npm run copy-assets",
    "copy-assets": "cp -r dist/* ../GG-JP/dist/public/",
    "standard": "standard || exit 0",
    "start": "webpack-dev-server",
    "test": "jest --watch",
    "testOnce": "jest"
  },
  "engines": {
    "node": "^9.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.40",
    "axios": "^0.17.1",
    "compression": "^1.7.2",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.42",
    "material-ui-icons": "^1.0.0-beta.36",
    "mime-types": "^2.1.18",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-swipeable-views": "^0.12.13",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.48",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.48",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.48",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.48",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.48",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.48",
    "@babel/preset-env": "^7.0.0-beta.48",
    "@babel/preset-react": "^7.0.0-beta.48",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-transform-imports": "^1.5.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "dotenv": "^5.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^22.4.2",
    "react-hot-loader": "^3.1.3",
    "standard": "^11.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cdn-plugin": "^2.0.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": ">=3.1.11",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^Actions/(.*)$": "<rootDir>/src/model/actions/$1",
      "^Components/(.*)$": "<rootDir>/src/view/components/$1",
      "^Constants/(.*)$": "<rootDir>/src/model/constants/$1",
      "^Containers/(.*)$": "<rootDir>/src/view/containers/$1",
      "^Data/(.*)$": "<rootDir>/src/view/data/$1",
      "^Icons/(.*)$": "<rootDir/node_modules/material-ui-icons/$1",
      "^Images/(.*)$": "<rootDir>/src/static/images/$1",
      "^Reducers/(.*)$": "<rootDir>/src/model/reducers/$1",
      "^Root/(.*)$": "<rootDir>/$1",
      "^UI/(.*)$": "<rootDir/node_modules/material-ui/$1",
      "^Utils$": "<rootDir>/src/utils/"
    }
  }
}
