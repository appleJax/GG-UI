{
  "name": "GameGogakuen-UI",
  "version": "1.0.0",
  "description": "UI for Viewing GameGogakuen Stats",
  "main": "index.js",
  "author": "appleJax <appleJax@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "deploy": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js --hot",
    "heroku-start": "npm run build && node server.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.31",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "babel-loader": "8.0.0-beta.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest-cli": "^22.1.4",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^Actions/(.*)$": "<rootDir>/src/actions/$1",
      "^Ajax$": "<rootDir>/src/ajax/'",
      "^Components/(.*)$": "<rootDir>/src/components/$1",
      "^Containers/(.*)$": "<rootDir>/src/containers/$1",
      "^Images/(.*)$": "<rootDir>/src/static/images/$1",
      "^Reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^UI/(.*)$": "<rootDir/node_modules/material-ui/$1",
      "^Utils$": "<rootDir>/src/utils/"
    }
  }
